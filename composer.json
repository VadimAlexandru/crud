{
    "name": "wtg-package/crud",
    "description": "pkg",
    "keywords": [
       "wtg"
    ],
    "homepage": "https://github.com/VadimAlexandru/crud",
    "license": "MIT",
    "authors": [
        {
            "name": "VadimAleksandru",
            "email": "v700.lg@gmail.com",
            "homepage": "https://github.com/VadimAlexandru",
            "role": "Creator & Maintainer"
        }
    ],
    "require": {
        "wtg-package/base": "1.1.13",
        "doctrine/dbal": "^2.5",
        "venturecraft/revisionable": "1.*",
        "intervention/image": "^2.3",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "^3.0",
        "orchestra/database": "3.8.x-dev",
        "spatie/laravel-translatable": "^3.1.3"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\CRUD\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\CRUD\\Tests\\": "tests"
        },
        "files": ["vendor/backpack/base/src/helpers.php"],
        "exclude-from-classmap": [
            "**/config"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Backpack\\CRUD\\CrudServiceProvider"
            ],
            "aliases": {
                "CRUD": "Backpack\\CRUD\\CrudServiceProvider"
            }
        }
    }
}
